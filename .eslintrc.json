{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "prettier",
    "eslint-plugin-prettier",
    "eslint-plugin-testing-library",
    "eslint-plugin-jest-dom"
  ],
  "rules": {
    "prettier/prettier": "error"
  }
}
